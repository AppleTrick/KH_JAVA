package com.test03;

public class PrnTest {
	/*
	*System.out.println("");
	*
	*System.out.print("");
	*
	*System.out.printf();
	*/
	public static void main(String[] args) {
		int i = 100;
		
		System.out.print("1. i : \\" +  i + "\n");
		System.out.println("2. i : \t" + i);
		System.out.printf("3. i : %10d \n",i);
		
		System.out.println("------------------");
		
		// 시험 점수 결과는 100점 이고, 학점은 'A' 가 나왔다.
		// 나의 오늘 감정지수는 90.50(d)% 이다.
		int su = 100;						//10자리
		char ch = 'A';						//5자리
		double d = 90.50d;				// 5자리 (소수점 2자리)
		// 		System.out.printf(); 사용
		
		System.out.printf("시험 점수 결과는 %d점 이고, 학점은 \'%c\' 가 나왔다. \n",su,ch);
		System.out.printf("나의 오늘 감정지수는 %d \% 이다",d);
	
	}
}
