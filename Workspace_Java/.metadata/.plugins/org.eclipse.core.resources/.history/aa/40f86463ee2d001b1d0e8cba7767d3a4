package com.test03;

public class PrnTest {
	/*
	*System.out.println("");
	*
	*System.out.print("");
	*
	*System.out.printf();
	*/
	public static void main(String[] args) {
		int i = 100;
		
		System.out.print("1. i : " +  i + "\n");
		System.out.println("2. i : \t" + i);
		System.out.printf("3. i : %d \n",i);
	
	}
}
