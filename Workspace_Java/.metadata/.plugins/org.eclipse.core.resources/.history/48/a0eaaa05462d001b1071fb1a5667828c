package com.test01;

public class Type04 {
	public static void main(String[] args) {
		// 문자형 char , 문자열 String ,  논리형 boolean
		
		//문자형
		char c1 = 'a';
		System.out.println(c1);
		char c2 = 'b';
		int i1 = c2;
		System.out.println(i1); // 아스키 code !!
		
		// 문자열
		String s1 = "aa";
		System.out.println(s1);
		
		
		
		
	}
}
