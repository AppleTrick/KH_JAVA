package com.test01;

public class Type02 {
	
	// 정수형 type : byte , short, int, long
	public static void main(String[] args) {
		
		// type  variable - literal;
		// 타입 변수 = 값;
		
		byte b01 = 126;
		System.out.println(b01);
		b01 = 111;
		System.out.println(b01);
		
		byte b02 = (byte) 128; // 형 변환 : casting
		System.out.println(b02);
	}

}
