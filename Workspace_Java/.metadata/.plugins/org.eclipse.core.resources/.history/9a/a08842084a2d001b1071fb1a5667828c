package com.test01;

public class Type05 {
	/*
	 * 
	 *   기본 타입											 			참조타입
	 * byte													->				Byte
	 * short												->				Short
	 * int													->				Int
	 * long													-> 			Long
	 * 
	 * float													->				Float
	 * double												->				Double
	 * 
	 * char													->				Character
	 * 
	 * boolean											->				Boolean
	 */
	
	public static void main(String[] args) {
		System.out.println("byte의 범위 : " + Byte.MIN_VALUE + " ~ " + Byte.MAX_VALUE);
	}
}
